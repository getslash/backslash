<div class="progress {{if is_running 'running'}}">
  <div class="progress-bar progress-bar-success" style={{successPercent}}>
  </div>
  <div class="progress-bar progress-bar-warning" style={{skipPercent}}>
  </div>
  <div class="progress-bar progress-bar-danger" style={{failedPercent}}>
  </div>
  {{#if is_running }}
    <div class="progress-bar progress-bar-striped active" style={{runningPercent}}></div>
  {{/if}}
</div>
