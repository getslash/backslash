{{#if (eq status "success")}}
  <i class="fa fa-check"></i>
{{else}}
  {{#if (eq status "skipped")}}
    <i class="fa fa-share"></i>
  {{else}}
    {{#if (eq status "running")}}
      <i class="fa fa-spin fa-circle-o-notch faint"></i>
    {{else}}
      <i class="fa fa-times"></i>
    {{/if}}
  {{/if}}

{{/if}}
