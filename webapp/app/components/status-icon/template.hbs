{{#elements/tool-tip title=status_lower}}
{{#if (eq status_lower "success")}}
  <i class="fa fa-check"></i>
{{else}}
  {{#if (eq status_lower "skipped")}}
    <i class="fa fa-share"></i>
  {{else}}
    {{#if (and (not session_model.end_time) (eq status_lower "running"))}}
      {{animated-spinner}}
    {{else}}
      {{#if (eq status_lower "planned")}}
        <i class="fa fa-ellipsis-h" aria-hidden="true"></i>
      {{else}}
        {{#if (eq status_lower "debugging") }}
          {{fa-icon "terminal"}}
        {{else}}
          <i class="fa fa-times"></i>
        {{/if}}
      {{/if}}
    {{/if}}
  {{/if}}

{{/if}}
{{/elements/tool-tip}}
